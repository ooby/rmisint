{
  "name": "rmisint",
  "version": "0.0.23",
  "description": "RMIS integration org",
  "main": "index.js",
  "scripts": {
    "start": "pm2 start index.js -i 1 --name rmisint --log-date-format 'YYYY-MM-DD HH:mm:ss'",
    "precommit": "lint-staged",
    "init-upload": "pm2 start initUpload.js -i 1 --name rmisint-upload --no-autorestart",
    "test": "mocha ./test --timeout 30000"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ooby/rmisint.git"
  },
  "keywords": [
    "RMIS",
    "integration",
    "org-server"
  ],
  "author": "Afanasy Argunov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ooby/rmisint/issues"
  },
  "homepage": "https://github.com/ooby/rmisint#readme",
  "dependencies": {
    "moment": "^2.18.1",
    "nconf": "^0.8.5",
    "refbooks": "^0.0.7",
    "rmisjs": "^0.5.2"
  },
  "devDependencies": {
    "mocha": "^3.5.3",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0"
  }
}
